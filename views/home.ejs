<%- include('partials/header'); -%>
<h1>Home</h1>
<p><%= homeStartingContent %></p>
<% posts.forEach(i => { %>
<h1><%= i.title %></h1>
<p>
  <%= i.content.substring(0, 100) + '...' %>
  <a href="/posts/<%=i.title%>">Read More</a>
</p>
<% }) %>
<button class="btn btn-outline-primary">
  <a href="/compose">Create New Post</a>
</button>
<%- include('partials/footer'); -%>
